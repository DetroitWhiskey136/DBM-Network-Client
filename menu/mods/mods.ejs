<div class="ConsoleDiv">
    <div class="ConsoleDivTab">
        <span class="ConsoleDivTabTitle">
            Console
        </span>
        <button class="ConsoleDivTabButtonShow Dbutton" onclick="$('#ModsConsoleLog').slideToggle(); $(this).html($(this).html() == 'Show' ? 'Hide' : 'Show')">Show</button>
        <button class="ConsoleDivTabButtonClear Dbutton" onclick="$('#ModsConsoleLog').html('')">Clear</button>
    </div>
    <textarea class="ConsoleLogText" id="ModsConsoleLog" readonly></textarea>
</div><br><br>
<div id="ModsDivTab">
    <p id="ModsDivTabTitle">
        <b>Mods</b>
    </p>
    <button class="Dbutton Dbutton_BlockStuff" id="ModsDivTabButtonUninstall" onclick="ModsUninstallClick()">
        Uninstall
    </button>
    <button class="Dbutton Dbutton_BlockStuff" id="ModsDivTabButtonInstall" onclick="ModsInstallClick()">
        Install
    </button>
    <select class="Dbutton Dbutton_BlockStuff" id="ModsBranchSelect">
        <option value="master" selected>Master</option>
        <option value="beta">Beta</option>
    </select>
</div>
<hr class="HRFirst">
<br>
<div class="StuffBelowScroll">
    <div id="ModsAltModsDiv">
        <div id="ModsAltModsDivSwitch">
            <label class="switch">
                <input id="ModsAM" type="checkbox" onclick="ModsAMClick()">
                <span class="slider round"></span>
            </label>
        </div>
        <div id="ModsAltModsDivText">
            <b>
                <span id="ModsAltModsTextFirst">
                    Install Alternative Mods
                </span>
            </b>
            <br>
            <span id="ModsAltModsTextSecond">When it's enabled, Additional Mods, which arenâ€™t used frequently, will be installed too</span>
        </div>
    </div>
</div>

<script src="../mods/mods.js"></script>